   "source": [
    "def dec(func):\n",
    "    def wrapper(*args):\n",
    "        print(\"before\")\n",
    "        func(*args)\n",
    "        print(\"after\")\n",
    "    return wrapper\n",
    "\n",
    "@dec\n",
    "def print_name(name):\n",
    "    print(name)"
   ]
  },
